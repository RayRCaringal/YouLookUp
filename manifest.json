{
  "name": "deletedvideos",
  "version": "1.0",
  "description": "Youtube Playlist Watcher Extension for deleted vidoes",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/playlist?list=*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Find Deleted Videoes."
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}